{
  "manifest_version": 2,
  "name": "e-gov stylist",
  "version": "1.0",
  "description": "A Chrome extension to customize elaws.e-gov.go.jp style",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://elaws.e-gov.go.jp/document*"],
      "css": ["style.css"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "32": "images/icon@32.png",
    "128": "images/icon@128.png"
  }
}
