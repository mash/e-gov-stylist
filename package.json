{
  "name": "e-gov-stylist",
  "version": "1.0.0",
  "description": "Customize e-gov style to what I like",
  "private": true,
  "type": "module",
  "dependencies": {
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.147",
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.6",
    "@types/node": "^16.3.3",
    "@types/puppeteer": "^5.4.4",
    "@types/tape": "^4.13.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "npm-check-updates": "^11.8.3",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "puppeteer": "^10.1.0",
    "tap-diff": "github:axross/tap-diff#44196f93a1e600c691fab7ffdbee88ccdae1a836",
    "tape": "^5.2.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore src --fix",
    "prettier": "prettier --write \"**/*.ts\"",
    "dev": "webpack --config webpack.config.development.cjs",
    "prod": "npm run clean && webpack --config webpack.config.production.cjs",
    "watch:dev": "webpack --config webpack.config.development.cjs --watch",
    "test": "ts-node --project tsconfig.test.json node_modules/tape/bin/tape test/test.ts | tap-diff",
    "watch:test": "tsc -w --project tsconfig.test.json",
    "clean": "rm dist/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "author": "mash <o.masakazu@gmail.com>",
  "license": "MIT"
}
